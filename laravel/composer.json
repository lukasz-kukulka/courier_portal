{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "backpack/crud": "v6.x-dev",
        "backpack/theme-coreuiv2": "^1.1",
        "backpack/theme-coreuiv4": "^1.0",
        "bordercloud/codemirror": "5.65.13",
        "elibyy/tcpdf-laravel": "^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "tecnickcom/tcpdf": "^6.6",
        "twbs/bootstrap-icons": "^1.10"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bordercloud/codemirror",
                "version": "5.65.13",
                "source": {
                    "url": "https://github.com/codemirror/CodeMirror",
                    "type": "git",
                    "reference": "5.65.13"
                },
                "autoload": {
                    "classmap": [
                        "lib/"
                    ]
                }
            }
        }
    ],
    "require-dev": {
        "backpack/generators": "^4.0",
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laracasts/generators": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "TCPDF\\": "vendor/tecnickcom/tcpdf/"
        },
        "files": [
            "app/Helpers/translate_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": ["php artisan storage:link"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
